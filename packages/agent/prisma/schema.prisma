datasource db {
  url      = env("DATABASE_URL")
  provider = "postgresql"
}

generator client {
  provider = "prisma-client-js"
}

model Document {
  id String @id @default(uuid()) @db.Uuid

  data       Json                     @db.JsonB
  dataVector Unsupported("TSVECTOR")?

  @@index([dataVector])
}
